package edu.emory.clinical.trials.webapp.server.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@SuppressWarnings("serial")
@Embeddable
public class ClinicalTrialMeshTermPrimaryKey implements Serializable {

	@Column(name = "nct_id")
	private String nctId;

	@Column(name = "term")
	private String meshTerm;

	public String getNctId() {
		return nctId;
	}
	
	public String getMeshTerm() {
		return meshTerm;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj) {
			return true;
		}

		if (obj == null) {
			return false;
		}

		if (getClass() != obj.getClass()) {
			return false;
		}

		ClinicalTrialMeshTermPrimaryKey other = (ClinicalTrialMeshTermPrimaryKey) obj;

		if (nctId == null) {
			if (other.nctId != null) {
				return false;
			}
		} 
		else if (!nctId.equals(other.nctId)) {
			return false;
		}

		if (meshTerm == null) {
			if (other.meshTerm != null) {
				return false;
			}
		} 
		else if (!meshTerm.equals(other.meshTerm)) {
			return false;
		}

		return true;
	}


	public int hashCode() {
		int code = 0;
		code += nctId == null ? 0 : nctId.hashCode();
		code += meshTerm == null ? 0 : meshTerm.hashCode();
		return code;

	}


}

